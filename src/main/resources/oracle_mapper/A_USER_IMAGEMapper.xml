<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.didispace.mybatis.a_user_image.A_USER_IMAGE_Mapper">



    <insert id="insertUserImage" parameterType="com.didispace.mybatis.a_user_image.A_USER_IMAGE">
        insert into A_USER_IMAGE(id,IMAGE)
        values (#{userImage.id,jdbcType=INTEGER},#{userImage.image,jdbcType=BLOB})
    </insert>







</mapper>
